// 구간별 합 구하기

int a;
int b;
int sum=0;

for(b=10;b<=100;b+=10)
  {
    for(a=b-9;a<=b;a++)
      {
        sum += a;
      }
  System.out.printf("%d~%d 까지 합 : %d \n",b-9,b,sum);
  }

->

int a;
int b;

for(b=10;b<=100;b+=10)
  {
    int sum=0;
    for(a=b-9;a<=b;a++)
      {
        sum += a;
      }
  System.out.printf("%d~%d 까지 합 : %d \n",b-9,b,sum);
  }

// sum값이 누적되어 더해지고 있었음
// 구간별 sum값을 분석해보면 해결이 쉬웠을 것
